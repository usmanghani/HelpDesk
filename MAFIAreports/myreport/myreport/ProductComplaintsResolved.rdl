  <DataSets>
    <DataSet Name="mysharedDS">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select product.productid as ProductID , count ( complaint.complaintid ) as ComplaintsResolved from product left outer join complaint on product.productid = complaint.productid where complaint.statusid = ( select statusid from status where statustext = 'resolved' ) group by product.productid;</CommandText>
        <DataSourceName>mysharedDS</DataSourceName>
      </Query>
      <Fields>
        <Field Name="ProductID">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>ProductID</DataField>
